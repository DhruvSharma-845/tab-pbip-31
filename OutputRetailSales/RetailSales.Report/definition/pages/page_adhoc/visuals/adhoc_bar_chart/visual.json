{
  "$schema": "https://developer.microsoft.com/json-schemas/fabric/item/report/definition/visualContainer/2.5.0/schema.json",
  "name": "adhoc_bar_chart",
  "position": {
    "x": 20,
    "y": 60,
    "z": 4,
    "height": 300,
    "width": 450,
    "tabOrder": 4
  },
  "visual": {
    "visualType": "clusteredBarChart",
    "autoSelectVisualType": true,
    "query": {
      "queryState": {
        "Category": {
          "projections": [
            {
              "field": {
                "Column": {
                  "Expression": {
                    "SourceRef": {
                      "Entity": "Retail Sales Data"
                    }
                  },
                  "Property": "Product Line"
                }
              },
              "queryRef": "Retail Sales Data.Product Line",
              "nativeQueryRef": "Product Line"
            }
          ]
        },
        "Y": {
          "projections": [
            {
              "field": {
                "Measure": {
                  "Expression": {
                    "SourceRef": {
                      "Entity": "Retail Sales Data"
                    }
                  },
                  "Property": "Total Sales"
                }
              },
              "queryRef": "Retail Sales Data.Total Sales",
              "nativeQueryRef": "Total Sales"
            }
          ]
        }
      }
    },
    "drillFilterOtherVisuals": true
  }
}
