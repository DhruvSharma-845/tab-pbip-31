{
  "$schema": "https://developer.microsoft.com/json-schemas/fabric/item/report/definition/visualContainer/2.5.0/schema.json",
  "name": "adhoc_bar_chart",
  "position": {
    "x": 20,
    "y": 170,
    "z": 5,
    "height": 300,
    "width": 640,
    "tabOrder": 5
  },
  "visual": {
    "visualType": "clusteredBarChart",
    "query": {
      "queryState": {
        "Category": {
          "projections": [
            {
              "field": {
                "Column": {
                  "Expression": {
                    "SourceRef": {
                      "Entity": "Retail Sales Data"
                    }
                  },
                  "Property": "Product Line"
                }
              },
              "queryRef": "Retail Sales Data.Product Line",
              "nativeQueryRef": "Product Line"
            }
          ]
        },
        "Y": {
          "projections": [
            {
              "field": {
                "Aggregation": {
                  "Expression": {
                    "Column": {
                      "Expression": {
                        "SourceRef": {
                          "Entity": "Retail Sales Data"
                        }
                      },
                      "Property": "Sales Amount"
                    }
                  },
                  "Function": 0
                }
              },
              "queryRef": "Sum(Retail Sales Data.Sales Amount)",
              "nativeQueryRef": "Total Sales"
            }
          ]
        }
      }
    },
    "drillFilterOtherVisuals": true,
    "autoSelectVisualType": false,
    "visualContainerObjects": {
      "title": [
        {
          "properties": {
            "show": {
              "expr": {
                "Literal": {
                  "Value": "true"
                }
              }
            },
            "text": {
              "expr": {
                "Literal": {
                  "Value": "'Total Sales by Product Line'"
                }
              }
            }
          }
        }
      ]
    }
  }
}
